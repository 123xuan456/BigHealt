<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:fresco="http://schemas.android.com/apk/res-auto"
    android:background="#EFEFF5"
    tools:context="dbighealth.bighealth.activity.EditdataActivity">

    <include layout="@layout/footer_tab_1"
        android:id="@+id/include8"></include>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="#ffff"
        android:layout_below="@+id/include8"
        android:orientation="vertical"
        android:id="@+id/linearLayout4">
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:padding="10dp"
            android:id="@+id/relativeLayout1"
            android:background="@drawable/biankuang_bottom"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="头像"
                android:textSize="18sp"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:id="@+id/textView54" />
            <com.facebook.drawee.view.SimpleDraweeView
                android:id="@+id/image"
                android:layout_width="60dp"
                android:layout_height="60dp"
                fresco:actualImageScaleType="centerInside"
                fresco:roundAsCircle="true"
                fresco:placeholderImage="@drawable/user"
                fresco:roundingBorderWidth="1dp"
                android:layout_alignTop="@+id/textView54"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="10dp"></com.facebook.drawee.view.SimpleDraweeView>

         <!--   <dbighealth.bighealth.view.RoundImageView
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:id="@+id/image"
                android:src="@drawable/cha"
                android:layout_alignTop="@+id/textView54"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="10dp" />-->
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:padding="10dp"
            android:id="@+id/relativeLayout2"
            android:background="@drawable/biankuang_bottom"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="昵称："
                android:textSize="18sp"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:id="@+id/textView55" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView59"
                android:gravity="center"
                android:layout_alignParentTop="true"
                android:layout_toEndOf="@+id/textView55"
                android:layout_alignParentBottom="true" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:padding="10dp"
            android:id="@+id/relativeLayout3"
            android:background="@drawable/biankuang_bottom"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="性别："
                android:textSize="18sp"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:id="@+id/textView56" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="18sp"
                android:id="@+id/sex"
                android:layout_marginEnd="16dp"
                android:layout_centerVertical="true"
                android:layout_toStartOf="@+id/imageView13" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/arrow"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="10dp"
                android:id="@+id/imageView13" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:padding="10dp"
            android:id="@+id/relativeLayout4"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="年龄："
                android:textSize="18sp"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:id="@+id/textView58" />

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="18"
                android:inputType="number"
                android:maxLength="2"
                android:padding="5dp"
                android:layout_marginEnd="10dp"
                android:textSize="18sp"
                android:background="@null"
                android:id="@+id/year"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true" />

        </RelativeLayout>
    </LinearLayout>
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:padding="10dp"
        android:background="#ffff"
        android:id="@+id/relativeLayout5"
        android:layout_marginTop="30dp"

        android:layout_below="@+id/linearLayout4"
        android:layout_alignParentStart="true">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="修改密码"
            android:textSize="18sp"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:id="@+id/textView57" />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/arrow"
            android:layout_marginEnd="10dp"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true" />
    </RelativeLayout>



</RelativeLayout>
