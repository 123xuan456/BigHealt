<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="dbighealth.bighealth.activity.SexActivity">
    <include layout="@layout/footer_tab_1"
        android:id="@+id/include10"></include>
<RelativeLayout
    android:id="@+id/rl"
    android:padding="5dp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@+id/include10"
    android:background="@drawable/biankuang_bottom"
    android:layout_alignParentEnd="true"
    android:layout_alignParentStart="true">
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="男"
    android:id="@+id/man"
    android:padding="10dp"
    />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView14"
        android:background="@drawable/ok3"
        android:layout_marginRight="10dp"
        android:layout_centerVertical="true"
        android:layout_alignParentEnd="true" />
</RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl1"
        android:layout_width="wrap_content"
        android:padding="5dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rl"
        android:background="@drawable/biankuang_bottom"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="女"
            android:id="@+id/woman"
            android:padding="10dp"
            />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView11"
            android:background="@drawable/ok3"
            android:layout_marginRight="10dp"
            android:visibility="gone"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true" />
    </RelativeLayout>


</RelativeLayout>
